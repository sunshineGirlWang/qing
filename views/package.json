{
    "name": "template",
    "version": "3.0.0",
    "dependencies": {
        "tmodjs": "1.0.4"
    },
    "tmodjs-config": {
        "output": "../../build",
        "charset": "utf-8",
        "syntax": "native",
        "helpers": "./template-helpers.js",
        "escape": true,
        "compress": true,
        "type": "default",
        "runtime": "template.js",
        "combo": true,
        "minify": true,
        "cache": false,
        "verbose": true
    }
}